@model Victuz.Models.Registration

@{
    ViewData["Title"] = "Uitschrijven";
}

<h1>Uitschrijven</h1>

<h3>Weet u zeker dat u zich wilt uitschrijven voor deze activiteit?</h3>
<div>
    <hr />
    <dl class="row">
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Uitschrijven" class="btn btn-danger" />
        <a asp-action="Index" class="btn btn-secondary ms-2">Terug naar overzicht</a>
    </form>
</div>
